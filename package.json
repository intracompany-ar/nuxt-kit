{
	"name": "@intracompany/nuxt-kit",
	"version": "1.0.1",
	"description": "Componenetes para app en nuxt",
	"keywords": [
		"commons",
		"nuxt"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/intracompany-ar/nuxt-kit.git"
	},
	"bugs": {
		"url": "https://github.com/intracompany-ar/nuxt-kit/issues"
	},
	"homepage": "https://github.com/intracompany-ar/nuxt-kit#readme",
	"license": "MIT",
	"author": "IntraCompany",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/types.d.ts",
			"import": "./dist/module.mjs",
			"require": "./dist/module.cjs"
		}
	},
	"main": "./dist/module.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"prepack": "nuxt-module-build build",
		"dev": "nuxi dev playground",
		"dev:build": "nuxi build playground",
		"dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
		"release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
		"lint": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
	},
	"dependencies": {
		"@nuxt/kit": "^3.16.1",
		"pinia": "^3.0.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@nuxt/devtools": "^2.3.2",
		"@nuxt/eslint-config": "^1.2.0",
		"@nuxt/module-builder": "^0.8.4",
		"@nuxt/schema": "^3.16.1",
		"@nuxt/test-utils": "^3.17.2",
		"@types/node": "latest",
		"changelogen": "^0.6.1",
		"eslint": "^9.23.0",
		"typescript": "~5.8.2",
		"vitest": "^3.0.9",
		"vue-tsc": "^2.2.8"
	},
	"peerDependencies": {
		"nuxt": "^3.16.1"
	}
}